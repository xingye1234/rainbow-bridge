{
  "extends": "@electron-toolkit/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "types": [
      "electron-vite/node"
    ],
    "paths": {
      "@common-types/*": [
        "src/types/*"
      ],
    }
  },
  "include": [
    "electron.vite.config.*",
    "src/main/**/*",
    "src/preload/**/*",
    "src/types/**/*"
  ]
}
